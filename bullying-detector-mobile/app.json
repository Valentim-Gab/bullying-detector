{
  "expo": {
    "name": "Bullying Detector",
    "slug": "bullying-detector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos/logomark.png",
    "backgroundColor": "#8f3d38",
    "primaryColor": "#8f3d38",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos/logomark.png"
      },
      "package": "bullying_detector.valentim.software"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logos/logomark-white.png",
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "#8f3d38"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7e9ee09e-74fe-4c89-8758-1c905f0f2970"
      }
    }
  }
}
