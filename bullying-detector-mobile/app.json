{
  "expo": {
    "name": "Bullying Detector",
    "slug": "bullying-detector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logos/logomark.png",
    "backgroundColor": "#8f3d38",
    "primaryColor": "#8f3d38",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logos/logomark.png"
      },
      "package": "software.valentim.bullying_detector"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logos/logomark-white.png",
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "#8f3d38"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "https://bullying-detector-api.valentim.software",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9b39789f-b511-41ac-b75d-7192405965fe"
      }
    },
    "owner": "valentim_gab",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9b39789f-b511-41ac-b75d-7192405965fe"
    }
  }
}
